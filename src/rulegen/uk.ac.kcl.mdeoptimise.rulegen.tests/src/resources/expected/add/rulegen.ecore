<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="rulegen" nsURI="http://uk.ac.kcl/rulegen/1.0" nsPrefix="rulegen">
  <eClassifiers xsi:type="ecore:EClass" name="Context">
    <eStructuralFeatures xsi:type="ecore:EReference" name="a" eType="#//A" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="b" eType="#//B" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="c" eType="#//C"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="d" eType="#//D" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="A">
    <eStructuralFeatures xsi:type="ecore:EReference" name="edgeAB" upperBound="-1"
        eType="#//B" eOpposite="#//B/edgeBA"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="B">
    <eStructuralFeatures xsi:type="ecore:EReference" name="edgeBA" upperBound="-1"
        eType="#//A" eOpposite="#//A/edgeAB"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="edgeBC" lowerBound="1"
        upperBound="-1" eType="#//C" eOpposite="#//C/edgeCB"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="C">
    <eStructuralFeatures xsi:type="ecore:EReference" name="edgeCB" lowerBound="1"
        upperBound="-1" eType="#//B" eOpposite="#//B/edgeBC"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="D">
    <eStructuralFeatures xsi:type="ecore:EReference" name="edgeDC" upperBound="-1"
        eType="#//C"/>
  </eClassifiers>
</ecore:EPackage>
